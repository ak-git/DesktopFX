<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean class="com.ak.comm.interceptor.simple.FixedFrameBytesInterceptor" scope="prototype">
        <constructor-arg name="baudRate" value="BR_460800"/>
        <constructor-arg name="frameLength" value="224"/>
    </bean>

    <bean class="com.ak.comm.converter.ToIntegerConverter" scope="prototype">
        <constructor-arg index="0">
            <bean class="java.lang.Class" factory-method="forName">
                <constructor-arg type="java.lang.String" value="com.ak.comm.converter.ADCVariable"/>
            </bean>
        </constructor-arg>
        <constructor-arg index="1" value="1000"/>
    </bean>

    <bean class="com.ak.comm.GroupService" destroy-method="close"/>

    <bean class="com.ak.fx.desktop.ViewController"/>
</beans>