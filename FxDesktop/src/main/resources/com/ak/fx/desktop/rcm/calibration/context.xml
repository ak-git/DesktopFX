<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean class="com.ak.comm.interceptor.rcm.RcmBytesInterceptor" scope="prototype"/>

    <bean class="com.ak.comm.converter.LinkedConverter" scope="prototype">
        <constructor-arg index="0">
            <bean class="com.ak.comm.converter.rcm.RcmConverter"/>
        </constructor-arg>
        <constructor-arg index="1" value="com.ak.comm.converter.rcm.calibration.RcmCalibrationVariable"/>
    </bean>


    <bean class="com.ak.comm.GroupService" destroy-method="close"/>

    <bean class="com.ak.fx.desktop.ViewController"/>
</beans>